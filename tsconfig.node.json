{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["ES2022"],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "noEmit": false,
    "paths": {
      "@server/*": ["./server/*"]
    }
  },
  "include": ["vite.config.ts", "server/**/*"],
  "exclude": ["node_modules"]
}
